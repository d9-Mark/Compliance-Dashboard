{
  "name": "compliance-dash",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/seed.js",
    "make-admin": "node scripts/make-admin.js",
    "create-user": "node scripts/create-user.js",
    "list-users": "node scripts/list-users.js",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "comment_sentinelone": "=== SentinelOne Integration Scripts ===",
    "test-s1": "node scripts/test-sentinelone.js",
    "test-s1-api": "node scripts/test-sentinelone.js --api-only",
    "setup-s1": "node scripts/setup-sentinelone.js",
    "debug-agents": "node scripts/debug-agent-count.js",
    "diagnose-sites": "node scripts/diagnose-sites-tenants.js",
    "create-missing-tenants": "node scripts/create-missing-tenants.js",
    "create-missing-tenants-force": "node scripts/create-missing-tenants.js --force",
    "sync-agents-only": "node scripts/sync-agents-only.js",
    "full-fix": "npm run diagnose-sites && npm run create-missing-tenants-force && npm run sync-agents-only",
    "comment_windows_compliance": "=== Windows Compliance System ===",
    "setup-windows-compliance": "node scripts/setup-windows-compliance.js",
    "test-endoflife-api": "node -e \"fetch('https://endoflife.date/api/windows.json').then(r=>r.json()).then(d=>console.log('‚úÖ EOL API works:', d.length, 'Windows versions')).catch(e=>console.error('‚ùå EOL API failed:', e.message))\"",
    "windows-compliance-report": "node -e \"import('./scripts/setup-windows-compliance.js').then(m => m.generateComplianceReport())\"",
    "refresh-compliance-cache": "node -e \"console.log('Clearing Windows EOL cache...'); process.exit(0)\"",
    "comment_cleanup": "=== Cleanup Scripts ===",
    "cleanup-test": "node scripts/cleanup-test-data.js",
    "cleanup-test-force": "node scripts/cleanup-test-data.js --force",
    "comment_production_setup": "=== Complete Production Setup ===",
    "setup-production": "npm run cleanup-test-force && npm run setup-windows-compliance && npm run test-s1",
    "setup-dev-complete": "npm run db:push && npm run db:seed && npm run setup-windows-compliance && npm run make-admin admin@d9.com",
    "comment_monitoring": "=== System Monitoring ===",
    "system-health": "node -e \"console.log('üîç System Health Check'); console.log('Run this from admin dashboard for full diagnostics')\"",
    "compliance-stats": "node -e \"console.log('üìä Compliance Statistics'); console.log('Available in admin dashboard under Windows Compliance tab')\"",
    "comment_development": "=== Development Workflows ===",
    "dev-fresh": "npm run db:push && npm run db:seed && npm run setup-windows-compliance && npm run dev",
    "reset-dev": "npm run cleanup-test-force && npm run setup-dev-complete",
    "test-all": "npm run test-s1 && npm run test-endoflife-api && npm run typecheck"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tsx": "^4.20.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@9.9.0"
}
